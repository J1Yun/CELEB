<link rel="stylesheet" href="/css/consumer/inquiryList.css" />
<div class="page-content page-container" id="page-content">
  <div class="padding">
    <div class="row">
      <div class="col">
        <div class="list list-row block">
          <% for (inquiry of inquiryList) { %>
          <a href="/store/<%= inquiry.storeId %>/inquiry" class="item-author text-color" data-abc="true">
            <div class="list-item">
              <div>
                <span class="w-48 avatar rounded"><img src="<%= inquiry.storeImage %>" /></span>
              </div>
              <div class="flex">
                <span class="item-author text-color" data-abc="true"><b><%= inquiry.storeName %></b></span>
                <div class="item-except text-muted text-sm h-1x"><%= inquiry.lastContents %></div>
              </div>
              <div class="no-wrap">
                <div class="item-date text-muted text-sm d-none d-md-block justify-content-end"><%= inquiry.createdAt %></div>
              </div>
            </div>
          </a>
          <% } %>
        </div>
      </div>
    </div>
  </div>
</div>
